run/main_scene="res://scenes/world.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

PlayerManager="*res://00_globals/player_manager.gd"
Focus="*res://00_globals/focus.gd"
